package linked_list;

import datastructure.ListNode;

public class merge_sort {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		ListNode a=new ListNode(Integer.MIN_VALUE);
		ListNode b=new ListNode(Integer.MIN_VALUE);
		
		for(int i=0;i<10;i++){
			
		}
		
		for(int i=0;i<5;i++){
			a.next=new ListNode(i);
		}
	}

	public static ListNode merge(ListNode a, ListNode b){
		ListNode list=new ListNode(Integer.MIN_VALUE);
		ListNode head=list;
		
		if(a==null && b==null) {
			return null;
			}
		else if(a!=null)
		{
			return b;
			}
		else if(b!=null)
		{
			return a;
			}
		else{
		
		while(a!=null && b!=null){
			if(a.val<=b.val){
				list.next=new ListNode(a.val);
				a=a.next;
				list=list.next;
			}
			else{
				list.next=new ListNode(b.val);
				b=b.next;
				list=list.next;
			}
		}
		
		if(a==null)
			list.next=b;
		else
			list.next=a;
		
		}
		return head.next;
	}
}
