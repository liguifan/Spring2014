package others;

public class removeDuplicates {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
			int[] a={1,2,2,2,3,4};
			for(int x:a){
			System.out.println(x);
			}
			removeDuplicates(a);
			for(int x:a){
				System.out.println(x);
				}
	}
	
	
	public static int removeDuplicates(int[] A){
		int len=0;
		int i=0;
        while(i<A.length){
        	if(A[len]==A[i]){
        		i++;
        	}
        	else
        	{
        		A[len+1]=A[i];
        			len++;
        			i++;
        	}
        	
		return len;
	}
        
        
	}
	

}
