package others;

public class decode_ways {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}
	
	
	public static boolean string_to_num(String str){
		int sum=0;
		for(int i=0;i<=str.length()-1;i++){
			sum=10*sum+((int)str.charAt(i)-48);
		}
		if(sum<=26)
			return true;
		else
			return false;
	}
}
