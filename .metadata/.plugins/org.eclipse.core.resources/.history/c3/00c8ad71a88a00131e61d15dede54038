package inheritence;

public class HourlyEmployee extends Employee{

	
	
	private double wageRate;
	private double hours;
	
	
	public HourlyEmployee() {
		super();
		this.wageRate=0;
		this.hours=0;
	}
	
	
	public HourlyEmployee(String theName, double a, double b){
		super(theName);
		wageRate=a;
		hours=b;
	}
}
