package others;

public class removeDuplicates {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
			int[] a={1,2,2,2,3,4};
			for(int x:a){
			System.out.println(x);
			}
			removeDuplicates(a);
			for(int x:a){
				System.out.println(x);
				}
	}
	
	
	public static void removeDuplicates(int[] A){
		int prev;
		int curr;
		int i=0;
		while(i<A.length-1){
			prev=A[i];
			curr=A[i+1];
			if(prev==curr){
				for(int j=i+1;j<A.length-1;j++){
					A[j]=A[j+1];
				}
				//should get rid of the last element of the array but how?
			}else
			{
				System.out.println("in");
				i++;
			}
		}
		
	}

}
