	}
	
	
	public ArrayList<Double> getWeightVector(ArrayList<ArrayList<Double>> data, ArrayList<Double> dataWeight) {
				int length=0;
				if(data==null)
					return null;
				else{
					length=data.get(0).size();
				}
				
				ArrayList<Double> sensorWeightVector = new ArrayList<Double>();   
		        for(int i = 0; i < length; i++) {  
		            sensorWeightVector.add(1d);  
		        } 
}
	
}
