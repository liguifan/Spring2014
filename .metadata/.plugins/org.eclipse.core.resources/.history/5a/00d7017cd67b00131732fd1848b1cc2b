package DP;

public class gas_station {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}
	
	public int canCompleteCircuit(int[] gas, int[] cost) {
        int j;
        int mark = 0;
        for(int i=0;i<gas.length;i++){
            int gas_left=0;
            for(j=i+1;j%gas.length!=i;j++){
                gas_left+=gas[j];
                gas_left-=cost[j];
                if(gas_left<0)
                    return -1;
                
            }
            mark=j;
        }
        return mark;
    }
}
