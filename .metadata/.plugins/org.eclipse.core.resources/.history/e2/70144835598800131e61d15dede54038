package heap_manipulation;

import java.util.Comparator;
import java.util.PriorityQueue;
import java.lang.*;
public class heap_implementation {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		PriorityQueue<Integer> queue=new PriorityQueue<Integer>();
		Comparator<Integer> less=new Less<Integer>;
		
		
	}
	
	public static <T> void pushHeap_min(T[] arr,int last, T item){
		int currPos,parentPos;
		
		currPos=last;
		parentPos=(currPos-1)/2;
		
		while(currPos!=0){
			if(((Comparator<T>)item).compareTo(arr[parentPos])){
				arr[currPos]=arr[parentPos];
				currPos=parentPos;
				parentPos=(currPos-1)/2;
			}
			else{
				break;
				
			}
			
			arr[currPos]=item;
		}
	}

}
