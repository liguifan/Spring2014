package others;

public class removeDuplicates {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}
	
	
	public static void removeDuplicates(int[] A){
		int prev;
		int curr;
		for(int i=0;i<A.length-1;i++){
			prev=A[i];
			curr=A[i+1];
			if(prev==curr){
				for(int j=i;j<A.length-1;j++){
					A[j]=A[j+1];
				}
			}
		}
		
	}

}
