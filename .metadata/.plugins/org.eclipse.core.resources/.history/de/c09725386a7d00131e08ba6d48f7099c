package arraylist;

public class search_a_2D_matrix {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int[] arr={1,2,3,4,5,6};
		
		boolean result=binary_search(arr, 5, 0, arr.length-1);
	}
	
	
	public static boolean search_matrix(int[][] matrix){
		
		for(int i=0;i<=matrix.length-1;i++){
			
		}
		
		
		
		
		return 
	}
	
	
	public static boolean binary_search(int[] matrix,int key, int a, int b){
		int len=b-a+1;
		int n=len/2;
		if(len==1){
			if(key==matrix[n]){
				return true;
			}
			else
			{
				return false;
			}
		}
		
		
		if(key<matrix[n]){
			return binary_search(matrix, key, a, n);
		}
		else{
			return binary_search(matrix, key, n+1, b);
		}	
		
	}

}
