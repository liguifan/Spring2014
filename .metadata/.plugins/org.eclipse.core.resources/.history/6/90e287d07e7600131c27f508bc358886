package array_manipulation;

public class array_manipulation {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] arr=new int[10];
		char[] arr2={'a','b','c','d'};
		for(int i=0;i<10;i++){
			arr[i]	=i;
			
		}
		System.out.println(arr2);
		System.out.println(new String(arr2));
	}
	
	
	

}
