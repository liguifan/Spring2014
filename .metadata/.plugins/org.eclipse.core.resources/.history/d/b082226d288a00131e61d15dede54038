		String s1="123";
		long s=System.nanoTime();
//		Integer.parseInt(s1);
		int x=parseInt(s1);
		long e=System.nanoTime();
		
		System.out.println(x);
	}
	
	
	public static int parseInt( final String s )
	{
	    int num  = 0;
	    final int len  = s.length( );
	    int i = 1;
	    while ( i < len )
	        num = num*10 + '0' - s.charAt( i++ );
	    return num;
	} 

}
