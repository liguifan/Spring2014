	}
	
	
	public static int read_class(String line){
		int y=(int)line.charAt(0)-48;
		return y;
	}
	// Pass the format like fxxx:xxxxe-01, return a tuple with first value is index and second value is its 
	// value for that index.
	public static Tuple extract_value(String single){
		String new_str=single.substring(1,single.length());
		String[] arr=new_str.split(":");
		int index=Integer.parseInt(arr[0]);
		double value=Double.parseDouble(arr[1]);
		Tuple result=new Tuple(index,value);
		return result;
	}
	
	
	
	

}
