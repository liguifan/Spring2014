	public static TreeNode build_tree1(int[] array,int start,int end){
		if(start>end)	return null;
		
		TreeNode root=new TreeNode(array[mid]);
		
		root.left=build_tree1(array,start,mid-1);
		root.right=build_tree1(array,mid+1,end);
		
	}

}
