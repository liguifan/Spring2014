	public static double dot_product(ArrayList<Double> weight,double[] b){
		double sum=0;
		for(int i=0;i<=a.length-2;i++){
			if(weight.get(i)==0 || b[i]==0){
				i++;
			}else{
				sum+=weight.get(i)*b[i];
			}
		}
