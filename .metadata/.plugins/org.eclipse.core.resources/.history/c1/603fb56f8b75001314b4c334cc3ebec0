	public static void Permu(String prefix,String str){
        if(str.length()==0){
            list.add(prefix);
            //System.out.println(prefix);
        }
        
        for(int i=0;i<str.length();i++){
            
            Permu(prefix+str.charAt(i),str.substring(0,i)+str.substring(i+1,str.length()));
            
        }
    }

}
