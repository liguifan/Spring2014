	public static void quicksort(int[] A,int p,int r){
		
		quicksort(A,q+1,r);
		
	}
	
	
	
	public static int partition(int[] A,int p,int r){
		int i=p-1;
		int x=A[r-1];
		int j=p;
		for(j=p;j<=r-1;j++){
			if(A[j]<=x){
				i++;
				int temp=A[j];
				A[j]=A[i];
				A[i]=A[j];
			}
		}
		
		return j+1
	}
	
