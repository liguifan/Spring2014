package array_manipulation;

public class removeDuplicates2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}
	
	public static int[] remove_dup(int a[]){
		int m=0;
		int s=1;
		int count=0;
		while(s<=a.length-1){
			
			if(a[m]==a[s]){
				count=1;
				m++;
				a[m]=a[s];
			}
			
			while(a[s]==a[m]){
				s++;
			}
			
			a[m+1]=a[s];
			m++;
			s++;
			count=0;
		}
		}
	}

}
