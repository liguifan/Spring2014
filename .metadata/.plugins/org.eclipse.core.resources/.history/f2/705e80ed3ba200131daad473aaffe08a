double[] learning_rate_with_importance(double importance, double x, int y, double[] theta,double learning_rate){
	
	double A=norm(x);
	double B=sparse_dot_product(theta, x);
	
	
	return theta;
}
	

public static double sparse_dot_product(double[] weight, Tuple tuple){
	
	double sum=0;
	while(tuple!=null){
		sum+=weight[tuple.index]*tuple.value;
		tuple=tuple.next;
	}
	return sum;
}

}
