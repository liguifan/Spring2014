	public static String longest_string(String s){
		
		
		
		
		
	}
	
	
	
	public static boolean isParlindrome(String s) {
        int left = 0, right = s.length() - 1;
        while(right >= left) {
            if(s.charAt(right) != s.charAt(left)) {
                return false;
            }
            right--;
            left++;
        }
        return true;
    }
}
